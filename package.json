{
  "name": "databrill-sparkline",
  "version": "1.0.0",
  "scripts": {
    "build": "vite build && tsc -p tsconfig.production.json",
    "lint": "tsc && eslint ./src/**/*.ts{,x} --fix",
    "postinstall": "rimraf .husky && husky install && husky add .husky/pre-commit \"yarn lint-staged\" && husky add .husky/commit-msg 'npx --no -- commitlint --edit \"$1\"' && husky add .husky/pre-push \"yarn test\"",
    "test": "jest"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@vitejs/plugin-react": "^1.1.0",
    "canvas": "^2.8.0",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.1",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.0",
    "typescript": "^4.5.2",
    "vite": "^2.6.14",
    "vite-tsconfig-paths": "^3.3.17"
  },
  "packageManager": "yarn@3.1.1",
  "files": [
    "dist"
  ],
  "main": "./dist/my-lib.umd.js",
  "module": "./dist/my-lib.es.js",
  "exports": {
    ".": {
      "import": "./dist/my-lib.es.js",
      "require": "./dist/my-lib.umd.js"
    }
  },
  "types": "dist/types/index.d.ts"
}
